{{! renders a json-schema "allOf"-definition.}}
<section class="json-schema-anyOf">
    {{! Display ref (i.e. superclasses)}}
    <dl>
        {{#each .}}
            <dt>
                {{>json-schema/datatype}}
            </dt>
            <dd>
                {{>json-schema/body $ref=""}}
            </dd>
        {{/each}}
    </dl>

</section>
