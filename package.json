{
  "publishConfig": {
    "tag": "next"
  },
  "name": "bootprint",
  "version": "1.0.1",
  "description": "Converts json and yaml into a static html page using Handlebars and Bootstrap",
  "main": "index.js",
  "bin": {
    "bootprint": "bin/bootprint.js"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "eslint --fix .",
    "test": "mocha && npm run lint",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha",
    "postcoverage": "istanbul check-coverage coverage/coverage.json --statements 100",
    "thought": "thought run -a",
    "version": "thoughtful changelog -o -a && npm run thought"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bootprint/bootprint.git"
  },
  "author": "Nils Knappmeier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bootprint/bootprint/issues"
  },
  "homepage": "https://github.com/bootprint/bootprint",
  "dependencies": {
    "commander": "^2.6.0",
    "customize-engine-handlebars": "^1.0.0",
    "customize-engine-less": "^1.0.1",
    "customize-engine-uglify": "^1.0.0",
    "customize-watch": "^1.0.0",
    "customize-write-files": "^1.0.0",
    "debug": "^2.1.2",
    "get-promise": "^1.3.1",
    "js-yaml": "^3.8.2",
    "live-server": "^1.2.0",
    "lodash": "^4.17.4",
    "q": "^1.4.1",
    "trace-and-clarify-if-possible": "^1.0.0"
  },
  "files": [
    "index.js",
    "bin/bootprint.js"
  ],
  "devDependencies": {
    "chai": "^3.4.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "m-io": "^0.5.0",
    "mocha": "^3.2.0",
    "thought": "^1.1.2",
    "thought-plugin-jsdoc": "^1.0.0",
    "thoughtful-release": "^0.3.1"
  }
}
