  },
  "bugs": {
    "url": "https://github.com/nknapp/customize-write-files/issues"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "pretest": "npm list --depth=0 -g standard || npm -g install standard",
    "test": "mocha && standard",
    "preformat": "npm list --depth=0 -g standard || npm -g install standard",
    "format": "standard --format",
    "thought": "thought run -a",
    "prethoughtcheck": "npm list --depth=0 -g thought || npm -g install thought",
    "thoughtcheck": "thought check-engines",
    "version": "npm run thought",
    "preversion": "npm run thoughtcheck"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "q": "^1.4.1",
    "q-io": "^1.13.1"
  },
