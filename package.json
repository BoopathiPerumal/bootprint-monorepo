    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "customize": "<1.0.0",
    "customize-engine-less": "<1.0.0"
  },
  "standard": {
    "ignore": [
      "spec/fixtures/**"
    ]
  },
  "keywords": []
