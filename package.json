{
  "publishConfig": {
    "tag": "next"
  },
  "name": "bootprint",
  "version": "1.0.1",
  "description": "Converts json and yaml into a static html page using Handlebars and Bootstrap",
  "main": "index.js",
  "bin": {
    "bootprint": "bin/bootprint.js"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "eslint --fix .",
    "test": "npm install bootprint-test-module && mocha && npm run lint",
    "coverage": "rm coverage -rf && npm install bootprint-test-module && istanbul cover --dir coverage/mocha ./node_modules/.bin/_mocha --print none",
    "postcoverage": "istanbul report --root coverage text-summary html  && istanbul check-coverage --statements 100",
    "thought": "thought run -a",
    "version": "thoughtful changelog -o -a && npm run thought"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootprint/bootprint.git"
  },
  "author": "Nils Knappmeier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bootprint/bootprint/issues"
  },
  "homepage": "https://github.com/bootprint/bootprint",
  "dependencies": {
    "commander": "^2.9.0",
    "customize": "^2.0.1",
    "customize-engine-handlebars": "^2.0.0",
    "customize-engine-less": "^2.0.0",
    "customize-write-files": "^2.0.0",
    "debug": "^2.6.3",
    "js-yaml": "^3.8.3",
    "pify": "^2.3.0",
    "popsicle": "^9.1.0",
    "popsicle-proxy-agent": "^3.0.0",
    "popsicle-status": "^2.0.0",
    "trace-and-clarify-if-possible": "^1.0.0"
  },
  "files": [
    "index.js",
    "bin/bootprint.js"
  ],
  "devDependencies": {
    "bootprint-test-module": "file:test/fixtures/bootprint-test-module",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nock": "bootprint/nock",
    "rimraf": "^2.6.1",
    "thought": "^1.1.2",
    "thought-plugin-jsdoc": "^1.0.0",
    "thoughtful-release": "^0.3.1",
    "trace-and-clarify-if-possible": "^1.0.0"
  },
  "directories": {
    "doc": "doc",
    "example": "examples",
    "test": "test"
  },
  "keywords": []
}
