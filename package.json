{
  "name": "customize-write-files",
  "version": "0.1.3",
  "description": "Post-processor that stores the result of a customize-run in a local directory",
  "repository": {
    "type": "git",
    "url": "https://github.com/bootprint/customize-write-files.git"
  },
  "author": {
    "name": "Nils Knappmeier",
    "email": "npm@knappi.org",
    "url": "https://github.com/nknapp"
  },
  "bugs": {
    "url": "https://github.com/bootprint/customize-write-files/issues"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "pretest": "standard --version || npm -g install standard",
    "test": "mocha && standard",
    "preformat": "standard --version || npm -g install standard",
    "format": "standard --format",
    "thought": "thought run -a",
    "prethoughtcheck": "thought --version || npm -g install thought",
    "thoughtcheck": "thought check-engines",
    "version": "thoughtful changelog -o -a && npm run thought",
    "preversion": "npm run thoughtcheck"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "q": "^1.4.1",
    "q-io": "^1.13.2"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "customize": "<1.0.0",
    "customize-engine-less": "<1.0.0",
    "ghooks": "^1.0.3",
    "mocha": "^2.2.5",
    "thoughtful-release": "^0.3.0"
  },
  "standard": {
    "ignore": [
      "spec/fixtures/**"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "standard"
    }
  },
  "keywords": []
}
